<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <template id="mystyle" name="My style" inherit_id="website.assets_frontend">
            <xpath expr="link[last()]" position="after">
                <link href="/frontend_form/static/less/header.less" rel="stylesheet" type="text/less"/>
                <!--<link href="/frontend_form/static/less/main.less" rel="stylesheet" type="text/less"/>-->
            </xpath>
        </template>

        <template id="myscripts" name="My scripts" inherit_id="website.assets_editor">
            <xpath expr="script[last()]" position="after">
                <script src="/frontend_form/static/js/alert.js" type="text/javascript"/>
            </xpath>
        </template>

    </data>
</odoo>